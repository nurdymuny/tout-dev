//
// Main Stylesheet
//

// Compass Plugins
// ===============
//@import compass/reset
//@import compass/css3

// Sass Features
// =============
//@import 0.site-settings
//@import 1.base
//@import 2.layout
//@import 3.states
//@import 4.themes
//@import variables
//@import mixins/all
//@import modules/all


header, footer, article, section, hgroup, nav, figure, aside
  display: block
  -webkit-transform: translateZ(0)

figure
  margin: 0
  padding: 0

input
  &[type=text], &[type=password], &[type=file], &[type=submit]
    -webkit-appearance: none
    -webkit-border-radius: 0
    border-radius: 0

textarea
  -webkit-appearance: none
  -webkit-border-radius: 0
  border-radius: 0

input[type=image]
  padding: 0
  border: none

body
  margin: 0
  background: #e5e4e4
  font: 12px / 16px Helvetica, Arial, sans-serif
  color: #6d6e70
  -webkit-text-size-adjust: 100%
  -ms-text-size-adjust: none
  min-width: 320px
  -webkit-transform: translateZ(0)

img
  border-style: none

a
  text-decoration: none
  color: #00a4e4
  &:hover
    text-decoration: underline
  &:active
    background-color: transparent

.ui-bar-a
  text-shadow: none

.ui-body-c, .ui-overlay-c
  background: #e5e4e4

/* home-pager

#home-page
  overflow: hidden
  position: relative
  z-index: 2

/* header

#header
  width: 100%
  z-index: 20
  -webkit-box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.1)
  box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.1)
  &.ui-bar-a
    background: none
    border: none

/* header-block

.header-block
  background: #41b0ed url(data:image/svg+xml;base64,)
  background: -moz-linear-gradient(top, #41b0ed 0%, #09669c 100%)
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #41b0ed), color-stop(100%, #09669c))
  background: -webkit-linear-gradient(top, #41b0ed 0%, #09669c 100%)
  background: -o-linear-gradient(top, #41b0ed 0%, #09669c 100%)
  background: -ms-linear-gradient(top, #41b0ed 0%, #09669c 100%)
  background: linear-gradient(to bottom, #41b0ed 0%, #09669c 100%)
  overflow: hidden
  border: solid
  border-width: 0 0 1px
  border-color: #393939
  padding: 6px 11px 5px 19px
  text-align: center

.logo
  display: inline-block
  vertical-align: top
  background: url(/assets/sprite.png) no-repeat
  width: 81px
  height: 33px
  text-indent: -9999px
  overflow: hidden
  a
    display: block
    height: 100%

#header .mobile-link
  float: left
  width: 20px
  height: 31px
  text-indent: -9999px
  overflow: hidden
  background: url(/assets/sprite.png) no-repeat -91px 0
  -webkit-border-radius: 0
  border-radius: 0
  -webkit-box-shadow: none
  box-shadow: none
  border: none
  position: relative
  top: 0
  left: 0
  margin: 0
  span
    display: none

.header-block .setting-link
  float: right
  background: url(/assets/sprite.png) no-repeat -194px -30px
  width: 29px
  height: 29px
  text-indent: -9999px
  overflow: hidden
  margin: 1px -1px 0 0

/* img-box

#header .img-box
  float: right
  -webkit-border-radius: 4px
  border-radius: 4px
  border: 1px solid #004a73
  background: #a4b2c3
  -webkit-box-shadow: 0 1px 0 0 rgba(255, 255, 255, 0.15)
  box-shadow: 0 1px 0 0 rgba(255, 255, 255, 0.15)
  overflow: hidden
  width: 31px
  img
    vertical-align: top

/* header-section

.header-section
  width: auto
  background: #d4d4d4
  border-bottom: 1px solid #b6b5b5
  position: relative
  z-index: 21
  &:after
    content: ''
    display: block
    clear: both
  .block
    overflow: hidden
    border-bottom: 1px solid #b6b6b6
    padding: 11px 60px 6px 9px
    text-shadow: 0 1px 0 #ececec
    color: #6d6e70
    -webkit-box-shadow: 0 1px 0 0 #ececec
    box-shadow: 0 1px 0 0 #ececec
    .partner-logo
      float: left
      margin: 0 13px 0 0
      width: 56px
      img
        display: block
        width: 100%
        height: auto
    .text-holder
      overflow: hidden
      padding: 2px 0 0
    h1
      font-size: 16px
      line-height: 16px
      color: #00a4e4
      margin: 0
    .title a
      text-decoration: none
      color: #00a4e4
      &:hover
        text-decoration: underline
    p
      margin: 0
  .gallery-block
    overflow: hidden
    padding: 10px 6px 13px 5px
    &.style02
      padding: 10px 6px 1px 5px
    .holder
      width: 100%
      position: relative
      padding: 7px 0 6px
      &:after
        content: ''
        display: block
        clear: both

.follow-link.ui-link
  float: right
  text-decoration: none
  color: #fff !important
  text-shadow: 0 1px 0 rgba(0, 0, 0, 0.69)
  padding: 5px 10px 5px 8px
  -webkit-border-radius: 4px
  border-radius: 4px
  border: solid
  border-width: 1px
  border-color: #1781bd #0e75ae #076598
  -webkit-box-shadow: inset 0 1px 0 0 rgba(255, 255, 255, 0.4)
  box-shadow: inset 0 1px 0 0 rgba(255, 255, 255, 0.4)
  letter-spacing: 0.2px
  background: #32b3fb
  background: -moz-linear-gradient(top, #32b3fb 0%, #097bbb 100%)
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #32b3fb), color-stop(100%, #097bbb))
  background: -webkit-linear-gradient(top, #32b3fb 0%, #097bbb 100%)
  background: -o-linear-gradient(top, #32b3fb 0%, #097bbb 100%)
  background: -ms-linear-gradient(top, #32b3fb 0%, #097bbb 100%)
  background: linear-gradient(to bottom, #32b3fb 0%, #097bbb 100%)
  font-size: 12px
  line-height: 17px

.header-section .gallery-block .holder .follow-link.ui-link
  margin: -7px 0 0 5px

.follow-link.ui-link
  &:hover, &:active
    color: #fff
    text-decoration: none
  .mark
    display: inline-block
    vertical-align: top
    font-size: 17px
    position: relative
    padding-right: 6px
    line-height: 17px

.header-section
  .info-text
    display: block
    overflow: hidden
    text-transform: uppercase
    font-size: 13px
    line-height: 16px
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.46)
    color: #6d6e70
    padding: 0 0 0 8px
    letter-spacing: -0.4px
  &.special
    .carousel, .info-text
      position: absolute
      top: -9999px
      left: -9999px
      visibility: hidden
    .block
      .text-holder
        position: absolute
        top: -9999px
        left: -9999px
        visibility: hidden
      padding: 0
    .gallery-block
      padding: 0
    .block
      float: left
      -webkit-box-shadow: none
      box-shadow: none
      border: none
    .gallery-block
      float: right
      .holder .follow-link.ui-link
        margin-top: 4px
    padding: 5px 6px 4px 9px
    .block .partner-logo
      width: 38px
    .gallery-block .holder
      padding: 0
  &.style02.special
    .block
      position: absolute
      top: -9999px
      left: -9999px
      visibility: hidden
    .gallery-block
      float: none
    .info-text
      position: relative
      top: 0
      left: 0
      visibility: visible
    .holder
      padding-top: 7px
    .gallery-block .holder .follow-link.ui-link
      margin-top: -7px
  .following
    float: left
    font-size: 16px
    line-height: 20px
    color: #555
    text-shadow: 1px 1px 0 #fff
    padding: 2px 0 2px 41px
    background: url(/assets/sprite.png) no-repeat -234px -34px
    margin: -2px 0 -5px 12px
    &.ui-link
      color: #555
      text-shadow: 1px 1px 0 #fff
    &:hover, &.ui-link:hover
      color: #555

/* carousel

.carousel
  background: #fff
  padding: 3px 46px 3px 3px
  border: 1px solid #cbcbcb
  -webkit-border-radius: 4px
  border-radius: 4px
  position: relative
  .mask
    overflow: hidden
  .slideset
    width: 99999px
    overflow: hidden
    .slide
      width: 42px
      float: left
      padding: 0 1px 0 0
      img
        vertical-align: top
  .btn-next
    position: absolute
    width: 40px
    height: 39px
    background: #e3e3e3 url(/assets/sprite.png) no-repeat -274px 13px
    top: 3px
    right: 4px
    text-indent: -9999px
    overflow: hidden

/* main

#main
  padding: 11px 7px
  position: relative
  z-index: 10
  &.style02
    background: #5e5e5e
    padding-top: 9px
  h2
    font-size: 12px
    line-height: 15px
    color: #00a4e4
    margin: 0 0 1px
    a
      text-decoration: none
      color: #00a4e4
      &:hover
        color: #00a4e4
        text-decoration: underline
  p
    margin: 0

/* content-block

.content-block
  margin: 0 0 6px
  border: 1px solid #c1c0c0
  -webkit-border-radius: 4px
  border-radius: 4px
  background: #fff
  padding: 6px 7px 0
  position: relative
  .holder
    overflow: hidden
    margin: 0 0 6px
    position: relative
    font-weight: bold
    color: #6d6e70
    .img-box
      float: left
      margin: 0 6px 0 0
      -webkit-border-radius: 3px
      border-radius: 3px
      border: 1px solid #c5c4c4
      background: #fff
      padding: 2px
      img
        vertical-align: top
    .text-holder
      overflow: hidden
    .time
      position: absolute
      top: 4px
      right: 4px
      font-style: normal
      font-size: 11px
      line-height: 13px
  .video-block
    overflow: hidden
    margin: 0 0 8px
    position: relative
    img
      display: block
      width: 100%
      height: auto
    .btn-play
      position: absolute
      top: 50%
      left: 50%
      width: 59px
      height: 59px
      margin: -31px 0 0 -29px
      background: url(/assets/sprite.png) no-repeat -126px 0
      z-index: 10
  .btn-holder
    overflow: hidden
    margin: 0 -7px
    padding: 5px 20px 6px 6px
    background: #ebebeb
    -webkit-border-radius: 0 0 3px 3px
    border-radius: 0 0 3px 3px
    border-top: 1px solid #dbdbdb
    -webkit-box-shadow: inset 0 1px 0 0 #fff
    box-shadow: inset 0 1px 0 0 #fff
    .link-back
      float: right
      width: 23px
      height: 17px
      background: url(/assets/sprite.png) no-repeat -194px 0
      margin: 6px 0 0
    .reply-link, .like-link
      float: left
      width: 90px
      border: solid
      border-width: 1px
      border-color: #c0c0c0 #bcbcbc #bfbfbf
      -webkit-border-radius: 3px
      border-radius: 3px
      margin: 0 8px 0 0
      color: #666
      text-shadow: 0 1px 0 #fff
      background: #fefefe url(data:image/svg+xml;base64,)
      background: -moz-linear-gradient(top, #fefefe 0%, #e1e1e1 100%)
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fefefe), color-stop(100%, #e1e1e1))
      background: -webkit-linear-gradient(top, #fefefe 0%, #e1e1e1 100%)
      background: -o-linear-gradient(top, #fefefe 0%, #e1e1e1 100%)
      background: -ms-linear-gradient(top, #fefefe 0%, #e1e1e1 100%)
      background: linear-gradient(to bottom, #fefefe 0%, #e1e1e1 100%)
    .reply-link a, .like-link a
      float: left
      width: 57px
      text-align: center
      border-right: 1px solid #b2b2b2
      padding: 5px 0 4px
      color: #666
      -webkit-border-radius: 2px 0 0 2px
      border-radius: 2px 0 0 2px
    .reply-link em, .like-link em
      float: left
      width: 31px
      border-left: 1px solid #fbfafa
      font-style: normal
      text-align: center
      padding: 5px 0 4px
      color: #666
      -webkit-border-radius: 0 2px 2px 0
      border-radius: 0 2px 2px 0

