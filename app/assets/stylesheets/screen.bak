/* slideshow

.slideshow
  overflow: hidden
  position: relative
  margin: -4px 0 -12px
  padding: 0 0 10px
  .slideset
    width: 100%
    overflow: hidden
    position: relative
    .slide
      position: absolute
      top: 0
      left: 0
      width: 100%
      padding: 3px
      z-index: 2
      background: #d4d4d4
      overflow: hidden
      -moz-box-sizing: border-box
      -webkit-box-sizing: border-box
      box-sizing: border-box
      &.active
        position: relative
        z-index: 3
  .placeholder
    float: left
    padding: 3px 4px 4px
    background: #fff
    -webkit-box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.3)
    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.3)
    -webkit-border-radius: 4px
    border-radius: 4px
    border: 1px solid #bdbcbc
    margin: 0 7px 0 0
    img
      vertical-align: top
  .text-holder
    overflow: hidden
    padding: 5px 0 0
  .follow-link
    float: right
    margin: 5px 0 0 10px
    -webkit-box-shadow: none
    box-shadow: none
  .title
    color: #555
    font-size: 16px
    line-height: 16px
    text-shadow: 1px 1px 0 #fff
  .email
    display: inline-block
    vertical-align: top
    color: #00a4e4
    text-shadow: 1px 1px 0 #fff
    &.ui-link
      display: inline-block
      vertical-align: top
      color: #00a4e4
      text-shadow: 1px 1px 0 #fff
    &:hover, &.ui-link:hover
      color: #00a4e4
  .info-text
    padding: 2px 0 7px
    overflow: hidden
    font-size: 11px
    line-height: 17px
    text-shadow: 1px 1px 0 #fff
    color: #555
    text-align: center
    text-transform: none
    letter-spacing: 0
    p
      margin: 0
  .switcher
    position: absolute
    bottom: 4px
    left: 50%
    margin: 0 0 0 -14px
    z-index: 10
    ul
      padding: 2px 0 0
      margin: 0
      list-style: none
      overflow: hidden
      li
        float: left
        margin: 0 2px 0 3px
      a
        display: block
        width: 7px
        height: 7px
        text-indent: -9999px
        overflow: hidden
        background: url(/assets/sprite.png) no-repeat -12px -51px
      .active a
        background-position: 0 -51px

